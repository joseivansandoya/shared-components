{
  "name": "@joseivansandoya/shared-components",
  "version": "1.0.0",
  "private": false,
  "main": "dist/index.js",
  "babel": {
    "presets": [
      ["@babel/preset-typescript", {
      "isTSX": true,
      "allExtensions": true
    }],
    "@babel/preset-react"
    ]
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.4",
